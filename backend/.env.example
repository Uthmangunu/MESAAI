# App (required — generate with: python -c "import secrets; print(secrets.token_hex(32))")
APP_SECRET_KEY=

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-role-key

# OpenRouter (one key — access Claude, GPT-4, Gemini, Mistral, etc.)
# Get your key at: https://openrouter.ai/keys
OPENROUTER_API_KEY=sk-or-...
# Optional: override default model (default: anthropic/claude-haiku-4-5)
# OPENROUTER_MODEL=anthropic/claude-sonnet-4-5

# Stripe
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...
STRIPE_PUBLISHABLE_KEY=pk_test_...

# Twilio
TWILIO_ACCOUNT_SID=ACxxx
TWILIO_AUTH_TOKEN=xxx
TWILIO_PHONE_NUMBER=+44...
TWILIO_WHATSAPP_NUMBER=whatsapp:+14155238886

# OpenAI (TTS for voice calls)
OPENAI_API_KEY=sk-...

# Google Calendar
GOOGLE_CLIENT_ID=xxx.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=xxx
GOOGLE_REDIRECT_URI=http://localhost:8000/api/integrations/google/callback

# Calendly
CALENDLY_API_KEY=xxx

# App
APP_ENV=development
FRONTEND_URL=http://localhost:5173
